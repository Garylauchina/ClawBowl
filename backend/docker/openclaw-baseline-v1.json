{
  "_comment": "ClawBowl OpenClaw 基础版配置模板 v1 (2026-02-13)",
  "_description": "每个用户容器的 OpenClaw 基础配置。模板变量在 config_generator.py 中渲染。",
  "_capabilities": [
    "LLM 聊天 (ZenMux -> GPT-4.1-mini / Claude Sonnet 4.5)",
    "Brave Web Search (搜索 + 网页抓取)",
    "无 Telegram/其他聊天渠道",
    "无本地命令执行权限",
    "无 Control UI"
  ],
  "_template_variables": {
    "ZENMUX_API_KEY": "ZenMux API 密钥 (来自 .env)",
    "MAX_TOKENS": "主模型最大输出 token (按订阅层级: free=1024, pro=4096, premium=8192)",
    "MAX_TOKENS_PREMIUM": "高级模型最大输出 token",
    "PRIMARY_MODEL": "主模型 ID (按订阅层级选择)",
    "GATEWAY_TOKEN": "每用户唯一的 Gateway 认证 token (自动生成)"
  },
  "models": {
    "providers": {
      "zenmux": {
        "baseUrl": "https://zenmux.ai/api/v1",
        "apiKey": "{{ ZENMUX_API_KEY }}",
        "api": "openai-completions",
        "models": [
          {
            "id": "openai/gpt-4.1-mini",
            "name": "GPT-4.1 mini via ZenMux",
            "reasoning": false,
            "input": ["text"],
            "cost": { "input": 0, "output": 0, "cacheRead": 0, "cacheWrite": 0 },
            "contextWindow": 200000,
            "maxTokens": "{{ MAX_TOKENS }}"
          },
          {
            "id": "anthropic/claude-sonnet-4.5",
            "name": "Claude Sonnet 4.5 via ZenMux",
            "reasoning": false,
            "input": ["text", "image"],
            "cost": { "input": 0, "output": 0, "cacheRead": 0, "cacheWrite": 0 },
            "contextWindow": 200000,
            "maxTokens": "{{ MAX_TOKENS_PREMIUM }}"
          }
        ]
      }
    }
  },
  "agents": {
    "defaults": {
      "model": { "primary": "{{ PRIMARY_MODEL }}" },
      "workspace": "/data/workspace",
      "maxConcurrent": 1,
      "subagents": { "maxConcurrent": 2 }
    }
  },
  "channels": {},
  "gateway": {
    "port": 18789,
    "mode": "local",
    "bind": "lan",
    "controlUi": { "enabled": false },
    "auth": {
      "mode": "token",
      "token": "{{ GATEWAY_TOKEN }}"
    },
    "tailscale": { "mode": "off" },
    "http": {
      "endpoints": {
        "chatCompletions": { "enabled": true },
        "responses": { "enabled": false }
      }
    },
    "nodes": { "denyCommands": ["*"] }
  },
  "tools": {
    "web": {
      "search": {
        "enabled": true,
        "apiKey": "BSAHUPck1u-lRrQYy7wQxcOqJsqd5LK"
      },
      "fetch": {
        "enabled": true
      }
    }
  },
  "commands": { "native": "auto", "nativeSkills": "auto" },
  "plugins": {},
  "skills": { "install": { "nodeManager": "npm" } }
}
