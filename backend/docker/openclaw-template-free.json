{
  "models": {
    "providers": {
      "zenmux": {
        "baseUrl": "https://zenmux.ai/api/v1",
        "apiKey": "{{ ZENMUX_API_KEY }}",
        "api": "openai-completions",
        "models": [
          {
            "id": "deepseek/deepseek-chat",
            "name": "DeepSeek V3.2",
            "reasoning": false,
            "input": ["text"],
            "cost": { "input": 0.28, "output": 0.42, "cacheRead": 0, "cacheWrite": 0 },
            "contextWindow": 128000,
            "maxTokens": {{ MAX_TOKENS }}
          },
          {
            "id": "xiaomi/mimo-v2-flash",
            "name": "小米 MiMo V2 Flash",
            "reasoning": true,
            "input": ["text"],
            "cost": { "input": 0, "output": 0, "cacheRead": 0, "cacheWrite": 0 },
            "contextWindow": 262144,
            "maxTokens": {{ MAX_TOKENS }}
          },
          {
            "id": "z-ai/glm-4.6v-flash-free",
            "name": "智谱 GLM 4.6V Flash",
            "reasoning": true,
            "input": ["text", "image"],
            "cost": { "input": 0, "output": 0, "cacheRead": 0, "cacheWrite": 0 },
            "contextWindow": 200000,
            "maxTokens": {{ MAX_TOKENS }}
          }
        ]
      }
    }
  },
  "agents": {
    "defaults": {
      "model": { "primary": "{{ PRIMARY_MODEL }}" },
      "imageModel": {
        "primary": "zenmux/z-ai/glm-4.6v-flash-free"
      },
      "models": {
        "zenmux/deepseek/deepseek-chat": {},
        "zenmux/xiaomi/mimo-v2-flash": {},
        "zenmux/z-ai/glm-4.6v-flash-free": {}
      },
      "workspace": "/data/workspace",
      "maxConcurrent": 2,
      "subagents": { "maxConcurrent": 2, "maxSpawnDepth": 2, "maxChildrenPerAgent": 3 }
    }
  },
  "channels": {},
  "gateway": {
    "port": 18789,
    "mode": "local",
    "bind": "loopback",
    "controlUi": { "enabled": true, "dangerouslyDisableDeviceAuth": true, "allowedOrigins": ["*"] },
    "auth": {
      "mode": "token",
      "token": "{{ GATEWAY_TOKEN }}"
    },
    "tailscale": { "mode": "off" },
    "http": {
      "endpoints": {
        "chatCompletions": { "enabled": true },
        "responses": { "enabled": false }
      }
    },
    "nodes": { "denyCommands": ["*"] }
  },
  "hooks": {
    "enabled": true,
    "token": "{{ HOOKS_TOKEN }}",
    "path": "/hooks",
    "defaultSessionKey": "hook:ingress",
    "allowRequestSessionKey": false
  },
  "tools": {
    "exec": {
      "applyPatch": { "enabled": true, "workspaceOnly": true }
    },
    "loopDetection": {
      "enabled": true,
      "warningThreshold": 10,
      "criticalThreshold": 20,
      "globalCircuitBreakerThreshold": 30
    },
    "web": {
      "search": { "enabled": false },
      "fetch": { "enabled": true }
    }
  },
  "browser": {
    "executablePath": "/usr/bin/chromium",
    "headless": true,
    "noSandbox": true
  },
  "commands": { "native": "auto", "nativeSkills": "auto" },
  "plugins": {
    "entries": {
      "llm-task": { "enabled": true }
    }
  },
  "skills": { "install": { "nodeManager": "npm" } }
}
